import java.util.Scanner;

public class CliReader{
	
	Scanner cliScanner;
	
	public CliReader() {
		cliScanner  = new Scanner(System.in);
	}
	
	private  String  readFromCLI() {
		final String command = cliScanner.nextLine();
		return command;
	}
	
	
	public void handlerForCliReader() {
		
	}
	
	public static void main(String[] args) {
		System.out.println("99".matches("[0-2]*[0-9]?[0-9]?"));
		System.out.println("99".matches("[^0-9]*[12]?[0-9]{1,2}[^0-9]*"));
		//"[^0-9]*[12]?[0-9]{1,2}[^0-9]*"
	}
}